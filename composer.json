{
    "name": "eventogy/crud-scaffold",
    "description": "Automatically creates all files required for a default CRUD cross-section",
    "keywords": [
        "Laravel",
        "Laravel Crud Generator"
    ],
    "homepage": "https://github.com/eventogy/crud-scaffold",
    "license": "MIT",
    "authors": [
        {
            "name": "Leslie Albrecht",
            "email": "rooirampokker@gmail.com,
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Rampokker\\CrudScaffold\\": "src/",
            "Rampokker\\CrudScaffold\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rampokker\\CrudScaffold\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelCrudGenerator\\LaravelCrudGeneratorServiceProvider"
            ],
            "aliases": {
                "LaravelCrudGenerator": "LaravelCrudGenerator\\Facades\\LaravelCrudGenerator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
